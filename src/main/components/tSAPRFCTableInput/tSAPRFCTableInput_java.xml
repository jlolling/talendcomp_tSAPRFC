<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="PROD"
    COMPATIBILITY="ALL"
    AUTHOR="Jan Lolling"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    HAS_CONDITIONAL_OUTPUTS="false"
    SUPPORTS_DB_TYPE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business/SAP</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR MULTI_SCHEMA="true" CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  	<PARAMETER
    	NAME="NOTE"
    	FIELD="LABEL"
    	NUM_ROW="10"
    >
    	<DEFAULT>(To use this component, you need first to add the SAP Java Connector (sapjco.jar or sapjco3.jar) in the Modules view)</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tSAPRFCConnection"
      NUM_ROW="20"
    />
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="30"/>
    <PARAMETER NAME="TABLE" FIELD="TEXT" REQUIRED="true" NUM_ROW="40" GROUP="TABLE_CONFIG"/>
    <PARAMETER NAME="FILTER" FIELD="TEXT" REQUIRED="false" NUM_ROW="50" GROUP="TABLE_CONFIG"/>
    <PARAMETER NAME="START_ROW" FIELD="TEXT" REQUIRED="false" NUM_ROW="60" GROUP="TABLE_CONFIG"/>
    <PARAMETER NAME="MAX_ROW_COUNT" FIELD="TEXT" REQUIRED="false" NUM_ROW="60" GROUP="TABLE_CONFIG"/>

  </PARAMETERS>    
  <ADVANCED_PARAMETERS>
  </ADVANCED_PARAMETERS>
  <CODEGENERATION>
  </CODEGENERATION>
  
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/>
    <RETURN AVAILABILITY="BEFORE" NAME="QUERY_DESC" TYPE="id_String"/>
  </RETURNS>
  
</COMPONENT>
