<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Jan Lolling"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business/SAP</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" GROUP="CONNECTION" NUM_ROW="20" REPOSITORY_VALUE="SAP" />
		
		<PARAMETER NAME="SERVERTYPE" FIELD="CLOSED_LIST" NUM_ROW="30" GROUP="CONNECTION" >
      		<ITEMS DEFAULT="APPLICATIONSERVER">
        		<ITEM NAME="APPLICATIONSERVER"   VALUE="APPLICATIONSERVER" />
        		<ITEM NAME="MESSAGESERVER"   VALUE="MESSAGESERVER" />
        	</ITEMS>
    	</PARAMETER>
    	
		<PARAMETER NAME="CLIENT" FIELD="TEXT" NUM_ROW="40" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="CLIENT">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USERID" FIELD="TEXT" NUM_ROW="50" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="USERID">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="60" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="PASSWORD">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="LANGUAGE" FIELD="TEXT" NUM_ROW="70" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="LANGUAGE">
			<DEFAULT>"EN"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="HOSTNAME" FIELD="TEXT" NUM_ROW="80" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="HOSTNAME">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SYSTEMNUMBER" FIELD="TEXT" NUM_ROW="90" GROUP="CONNECTION" REQUIRED="true" REPOSITORY_VALUE="SYSTEMNUMBER" SHOW_IF="SERVERTYPE=='APPLICATIONSERVER'">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SYSTEMID" FIELD="TEXT" NUM_ROW="100" GROUP="CONNECTION" REQUIRED="true" SHOW_IF="SERVERTYPE=='MESSAGESERVER'">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="GROUPNAME" FIELD="TEXT" NUM_ROW="110" GROUP="CONNECTION" REQUIRED="true" SHOW_IF="SERVERTYPE=='MESSAGESERVER'">
			<DEFAULT>"Public"</DEFAULT>
		</PARAMETER>
		
<!--        <PARAMETER NAME="SAP_PROPERTIES" FIELD="TABLE" REQUIRED="false" NUM_ROW="160" NB_LINES="3" REPOSITORY_VALUE="SAP_PROPERTIES">
	        <ITEMS>
	            <ITEM NAME="PROPERTY" />
	            <ITEM NAME="VALUE" />
	        </ITEMS>
	    </PARAMETER>
-->	              
	</PARAMETERS>
